{
  "name": "@masilia/consent-bundle-react",
  "version": "1.0.0",
  "description": "React components for Masilia Consent Bundle",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:types && npm run build:js && npm run build:css",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c",
    "build:css": "npm run copy:css",
    "copy:css": "mkdir -p dist/styles && cp src/styles/*.css dist/styles/",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "keywords": [
    "react",
    "consent",
    "cookies",
    "gdpr",
    "privacy",
    "cookie-banner",
    "cookie-consent",
    "symfony"
  ],
  "author": "Masilia Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/masilia/consent-bundle.git",
    "directory": "assets"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "rollup": "^4.9.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
