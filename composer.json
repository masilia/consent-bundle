{
    "name": "masilia/consent-bundle",
    "description": "Modern Symfony bundle for cookie consent management with React frontend and Ibexa Admin integration",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Masilia Team",
            "email": "dev@masilia.com"
        }
    ],
    "require": {
      "php": "^8.0 || ^8.1 || ^8.2",
      "ibexa/core": "^4.6",
      "ibexa/admin-ui": "^4.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0",
        "symfony/phpunit-bridge": "^5.4|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Masilia\\ConsentBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Masilia\\ConsentBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
